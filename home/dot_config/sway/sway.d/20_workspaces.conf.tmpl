#workspace $ws4 output $primary $secondary $tertiary
#workspace $ws5 output $primary $secondary $tertiary
#workspace $ws6 output $tertiary $secondary $primary
#workspace $ws7 output $tertiary $secondary $primary
#workspace $ws8 output $tertiary $secondary $primary
#workspace $ws9 output $tertiary $secondary $primary
#workspace $ws10 output $tertiary $secondary $primary

#workspace $wsa1 output $tertiary $secondary $primary
#workspace $wsa2 output $tertiary $secondary $primary
#workspace $wsa3 output $tertiary $secondary $primary
#workspace $wsa4 output $tertiary $secondary $primary
#workspace $wsa5 output $tertiary $secondary $primary
#workspace $wsa6 output $tertiary $secondary $primary
#workspace $wsa7 output $tertiary $secondary $primary
workspace $wsa8 output $tertiary $primary $secondary
workspace $wsa9 output $tertiary $primary $secondary
workspace $wsa10 output $primary $secondary $tertiary

